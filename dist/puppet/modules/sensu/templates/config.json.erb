{
  "rabbitmq": {
    "ssl": {
      "private_key_file": "<%= client_key %>",
      "cert_chain_file": "<%= client_cert %>"
    },
    "host": "<%= rabbitmq_host %>",
    "port": <%= rabbitmq_port %>,
    "user": <%= rabbitmq_user %>,
    "password": <%= rabbitmq_password %>,
    "vhost": <%= rabbitmq_vhost %>
  },
  "redis": {
    "host": "<%= redis_host %>",
    "port": <%= redis_port %>
  },
  "api": {
    "host": "<%= api_host %>",
    "port": <%= api_port %>
  },
  "dashboard": {
    "host": "<%= dashboard_host %>",
    "port": <%= dashboard_port %>,
    "user": "<%= dashboard_user %>",
    "password": "<%= dashboard_password %>"
  }
}
